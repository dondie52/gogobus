// ============================================
// 1. IMPORT THE NEW TRACKING PAGE
// ============================================
import Tracking from './pages/tracking/Tracking';

// ============================================
// 2. IMPORT TRACKING CONTEXT (OPTIONAL)
// ============================================
import { TrackingProvider } from './context/TrackingContext';

// ============================================
// 3. ADD ROUTE TO YOUR ROUTER
// ============================================

/*
Add this route inside your <Routes> component:

<Route
  path="/tracking/:routeId"
  element={
    <ProtectedRoute>
      <Tracking />
    </ProtectedRoute>
  }
/>

FULL EXAMPLE (adjust to match your existing structure):
*/

// Example router structure:
import { BrowserRouter, Routes, Route } from 'react-router-dom';
import ProtectedRoute from './components/ProtectedRoute';
// ... other imports

function AppRoutes() {
  return (
    <Routes>
      {/* ... existing routes ... */}
      
      {/* Live Tracking Route */}
      <Route
        path="/tracking/:routeId"
        element={
          <ProtectedRoute>
            <Tracking />
          </ProtectedRoute>
        }
      />
      
      {/* ... other routes ... */}
    </Routes>
  );
}

// ============================================
// 4. WRAP APP WITH TRACKING PROVIDER (OPTIONAL)
// ============================================

/*
If you want to use the TrackingContext across your app,
wrap your providers like this:

function App() {
  return (
    <BrowserRouter>
      <AuthProvider>
        <TrackingProvider>
          <AppRoutes />
        </TrackingProvider>
      </AuthProvider>
    </BrowserRouter>
  );
}
*/

